<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="fastentity.command.fast.entity" class="Jonathankablan\Bundle\FastEntityBundle\Command\MagicEntityCommand" public="true">
            <tag name="console.command" command="magic:entity" />
        </service>

        <service id="fastentity.config.manager" class="Jonathankablan\Bundle\FastEntityBundle\Configuration\ConfigManager" public="true">
            <argument>%fastentity.config%</argument>
            <argument>%kernel.debug%</argument>
        </service>
    </services>
</container>
